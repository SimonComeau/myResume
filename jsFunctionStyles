// Javascript functions styles 
function name (parameter) {
	code
}

var name = function (parameter) {
	code
}

name (parameter) => {
	code
}

var name = (parameter) => {
	code
}


function name (parameter1, parameter2) {
	return parameter1 + parameter2
}

var name = function (parameter1, parameter2) {
	return parameter1 + parameter2
}

name (parameter1, parameter2) => parameter1 + parameter2

var name = (parameter1, parameter2) => parameter1 + parameter2
